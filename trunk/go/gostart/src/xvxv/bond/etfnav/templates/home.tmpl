<!DOCTYPE html>
<html>
  <head>
    <title>CSOP China 5-Year Treasury Bond</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link rel="stylesheet" href="/css/bootstrap.min.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="http://cdn.bootcss.com/html5shiv/3.7.0/html5shiv.min.js"></script>
        <script src="http://cdn.bootcss.com/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
  <div> </div>
  <div class="container">
    <div class="btn-group btn-group-lg">
      <button type="button" class="btn btn-default active">China 5-Year Treasury Bond</button>
      <button type="button" class="btn btn-default">FTSE China A50 ETF</button>
      <button type="button" class="btn btn-default">CES China A80 ETF</button>
    </div>
    <p></p>
    <div id="content">

    </div>
  </div>

<script type="text/javascript" src="/js/jquery-1.11.0.min.js"></script>
<script type="text/javascript">
var infoType = 0;
var timestamp = (new Date()).valueOf();
function loadContent(){
  $("#content").load("/indexInfo/"+infoType+"?timestamp="+(timestamp++));
}
$(function(){
  window.setInterval("loadContent();",10000);
  $(".btn-group button").click(function(){
    $("button.active").removeClass("active");
    document.title=$(this).html();
    $(this).addClass("active");
    infoType = $(this).index();
    loadContent();
  });
  loadContent();
});
</script>

  </body>
</html>