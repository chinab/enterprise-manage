<?xml version="1.0" encoding="UTF-8"?>
<project>
	<parent>
		<artifactId>enmboot</artifactId>
		<groupId>com.lineboom.enm.enmboot
		</groupId>
		<version>1.0-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.lineboom.enm.enmboot
	</groupId>
	<artifactId>enmboot-webapp</artifactId>
	<name>enmboot Webapp</name>
	<packaging>war</packaging>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.0.2</version>
				<executions>
					<execution>
						<id>skinny-war</id>
						<goals>
							<goal>war</goal>
						</goals>
						<phase>post-integration-test</phase>
						<inherited>false</inherited>
						<configuration>
							<warSourceExcludes>
								WEB-INF/lib/**
							</warSourceExcludes>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>com.lineboom.enm.enmboot
			</groupId>
			<artifactId>esboot-app</artifactId>
			<version>${pom.version}</version>
		</dependency>

		<dependency>
			<groupId>com.lineboom.enm.enmcrm
			</groupId>
			<artifactId>enmcrm-app</artifactId>
			<version>${pom.version}</version>
		</dependency>
		<dependency>
			<groupId>com.lineboom.enm.enmcrm
			</groupId>
			<artifactId>enmcrm-webapp</artifactId>
			<version>${pom.version}</version>
			<type>war</type>
		</dependency>

		<dependency>
			<groupId>com.lineboom.enm.enmerp
			</groupId>
			<artifactId>enmerp-app</artifactId>
			<version>${pom.version}</version>
		</dependency>
		<dependency>
			<groupId>com.lineboom.enm.enmerp
			</groupId>
			<artifactId>enmerp-webapp</artifactId>
			<version>${pom.version}</version>
			<type>war</type>
		</dependency>

		<dependency>
			<groupId>com.lineboom.enm.enmfm
			</groupId>
			<artifactId>enmfm-app</artifactId>
			<version>${pom.version}</version>
		</dependency>
		<dependency>
			<groupId>com.lineboom.enm.enmfm
			</groupId>
			<artifactId>enmfm-webapp</artifactId>
			<version>${pom.version}</version>
			<type>war</type>
		</dependency>

		<dependency>
			<groupId>com.lineboom.enm.enmom
			</groupId>
			<artifactId>enmom-app</artifactId>
			<version>${pom.version}</version>
		</dependency>
		<dependency>
			<groupId>com.lineboom.enm.enmom
			</groupId>
			<artifactId>enmom-webapp</artifactId>
			<version>${pom.version}</version>
			<type>war</type>
		</dependency>

		<dependency>
			<groupId>com.lineboom.enm.enmsm
			</groupId>
			<artifactId>enmsm-app</artifactId>
			<version>${pom.version}</version>
		</dependency>
		<dependency>
			<groupId>com.lineboom.enm.enmsm
			</groupId>
			<artifactId>enmsm-webapp</artifactId>
			<version>${pom.version}</version>
			<type>war</type>
		</dependency>
		
	</dependencies>
</project>